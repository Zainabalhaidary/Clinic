                            <WrapGrid Orientation="Horizontal"/>

                        </ItemsPanelTemplate>
                    </GridView.ItemsPanel>
                    <GridView.ItemTemplate>
                        <DataTemplate>
                            <!--<Border BorderBrush="Gray" BorderThickness="1" >-->
                            <Grid 
                             
                                Grid.Row="2" Height="60" Width="123.4" Background="{Binding Color}">

                                <!--<TextBlock Text="{Binding Name}" VerticalAlignment="Top"/>-->
                                <TextBlock Text="{Binding Value}" VerticalAlignment="Top" Foreground="Gray"/>
                                <TextBlock Text="{Binding Info}" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Gray" FontSize="10"/>
                                <!--<TextBlock Text="History Info Avalable" VerticalAlignment="Center" Foreground="Gray" FontSize="10"/>-->
                            </Grid>

                            <!--</Border>-->
                        </DataTemplate>
                    </GridView.ItemTemplate>

                </GridView>
                <Popup x:Name="LightDismissAnimatedPopup" IsLightDismissEnabled="True"  >
                    <Popup.ChildTransitions>
                        <TransitionCollection>
                            <PopupThemeTransition />
                        </TransitionCollection>
                    </Popup.ChildTransitions>
                    <Border BorderBrush="Gray" BorderThickness="2" Background="White" Width="200" Height="200">
